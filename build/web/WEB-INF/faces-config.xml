<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <from-view-id> /Authentification/auth.xhtml</from-view-id>
        <navigation-case>
            <from-outcome >
                admin
            </from-outcome>
            <to-view-id>
                admin/index.xhtml
            </to-view-id>
        </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
        <from-view-id> /Authentification/auth.xhtml</from-view-id>
        <navigation-case>
            <from-outcome >
                user
            </from-outcome>
            <to-view-id>
                user/index.xhtml
            </to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id> /Authentification/auth.xhtml</from-view-id>
        <navigation-case>
            <from-outcome >
                auth
            </from-outcome>
            <to-view-id>
                /Authentification/auth.xhtml
            </to-view-id>
        </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
        <from-view-id>/Authentification/auth.xhtml</from-view-id>
        <navigation-case>
            <from-outcome >
                index
            </from-outcome>
            <to-view-id>
                admin/index.xhtml
            </to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
</faces-config>
